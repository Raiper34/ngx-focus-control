{
  "name": "ngx-focus-control-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:lib": "ng build ngx-focus-control --configuration production && copy-files-from-to",
    "test": "ng test",
    "test:lib": "ng test ngx-focus-control --code-coverage",
    "test:lib:coveralls": "npm run test:lib && cat ./coverage/lcov.info | coveralls",
    "test:lib:dev": "ng test ngx-focus-control --watch=true --browsers=Chrome",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "lint:lib": "ng lint ngx-focus-control",
    "lint:lib:fix": "ng lint ngx-focus-control --fix",
    "e2e": "ng e2e",
    "deploy": "npm run build && netlify deploy --dir=dist/ngx-focus-control-app --prod",
    "publish": "npm run build:lib && cd dist/ngx-focus-control && npm publish"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.1.4",
    "@angular/common": "~9.1.4",
    "@angular/compiler": "~9.1.4",
    "@angular/core": "~9.1.4",
    "@angular/forms": "~9.1.4",
    "@angular/platform-browser": "~9.1.4",
    "@angular/platform-browser-dynamic": "~9.1.4",
    "@angular/router": "~9.1.4",
    "bulma": "^0.9.3",
    "ngx-highlightjs": "^4.1.4",
    "npm-ls-scripts": "^0.1.2",
    "rxjs": "~6.5.4",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.4",
    "@angular-devkit/build-ng-packagr": "~0.901.15",
    "@angular/cli": "~9.1.4",
    "@angular/compiler-cli": "~9.1.4",
    "@angular/language-service": "~9.1.4",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.20.37",
    "@types/puppeteer": "^5.4.4",
    "codelyzer": "^5.1.2",
    "copy-files-from-to": "^3.2.2",
    "coveralls": "^3.1.1",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~3.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "ng-packagr": "^9.0.0",
    "protractor": "~5.4.3",
    "puppeteer": "^10.4.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~3.8.3"
  },
  "copyFiles": [
    {
      "from": "README.md",
      "to": "dist/ngx-focus-control/README.md"
    },
    {
      "from": "CHANGELOG.md",
      "to": "dist/ngx-focus-control/CHANGELOG.md"
    },
    {
      "from": "logo.svg",
      "to": "dist/ngx-focus-control/logo.svg"
    }
  ]
}
